{
  "name": "leitsch/kirby-uniform-hcaptcha",
  "description": "Kirby 3 hCaptcha guard for the Uniform plugin",
  "type": "kirby-plugin",
  "require": {
    "php": "^7.4|^8.0",
    "getkirby/composer-installer": "^1.1",
    "mzur/kirby-uniform": "^4.0"
  },
  "license": "MIT",
  "authors": [{
    "name": "Lukas Leitsch",
    "email": "lukas@leitsch.org"
  }],
  "autoload": {
    "psr-4": {
      "Uniform\\Guards\\": "src/Guards"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "installer-name": "uniform-hcaptcha"
  }
}
